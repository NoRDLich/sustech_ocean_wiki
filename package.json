{
  "name": "team-slug",
  "version": "0.0.1",
  "license": "CC-BY-4.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:light": "npm run pre-build:backup-large && tsc && vite build && npm run post-build:restore-large",
    "build:ci": "npm run pre-build:backup-large && tsc && vite build",
    "pre-build:backup-large": "mkdir -p .build-tmp && (mv public/static/sfp-lineplot .build-tmp/ 2>/dev/null || true) && (mv public/static/yonE-lineplot .build-tmp/ 2>/dev/null || true)",
    "post-build:restore-large": "(mv .build-tmp/sfp-lineplot public/static/ 2>/dev/null || true) && (mv .build-tmp/yonE-lineplot public/static/ 2>/dev/null || true) && (rmdir .build-tmp 2>/dev/null || true)",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,css}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@types/katex": "^0.16.7",
    "bootstrap": "^5.3.3",
    "katex": "^0.16.22",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-katex": "^3.1.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-katex": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
